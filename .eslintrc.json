{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "quotes":[2, "single", { "avoidEscape": true }],
        "indent":2,
        "react/no-unescaped-entities":1,
        "react/prop-types":0,
        "react/react-in-jsx-scope":0
    }
}
